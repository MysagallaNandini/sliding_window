class Solution:
    def climbStairs(self, n: int) -> int:
        def solve(n, dp):
            if n == 0:
                return 1
            if n < 0:
                return 0
            if dp[n] == -1:
                dp[n] = solve(n - 1, dp) + solve(n - 2, dp)
            return dp[n]
        dp = [-1] * (n + 1)
        return solve(n, dp)
